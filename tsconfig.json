{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "compilerOptions": {
    "plugins": [{ "name": "next" }],
    "composite": false,
    "declaration": false,
    "declarationMap": false,
    "incremental": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "inlineSources": false,
    "isolatedModules": true,
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "preserveWatchOutput": true,
    "skipLibCheck": true,
    "strict": false,
    "jsx": "preserve",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "noEmit": true,
    "resolveJsonModule": true,
    "target": "es5",
    "paths": {
      "@/*": ["./*"],
      "xy-ui": ["./components/xy-ui"]
    },
    "baseUrl": "src/",
    "allowJs": true
  },
  "include": [
    "src",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "theme.config.jsx",
    "tailwind.config.ts"
  ],
  "exclude": ["node_modules", "pro-examples"]
}
